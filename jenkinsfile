pipeline{
    agent any
    stages{
        stage('code package'){
            steps{
                mvnpath = tool name: 'maven3', type: 'maven'
                sh script: "${mvnpath}/mvn clean package"
            }
        }
        stage('docker compose'){
            steps{
                sh script: 'docker-compose up -d'
            }
        }
    }
}
